{
  parserOptions: {
    ecmaVersion: 6,
    ecmaFeatures: {
      jsx: true
    },
    sourceType: "module"
  },
  rules: {
    "comma-dangle": [2, "always-multiline"],
    "camelcase": [0, {"properties": "never"}]
  },
  extends: [
    "standard",
    "standard-react"
  ],
  plugins: [
    "standard",
    "react"
  ]
}
