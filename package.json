{
  "name": "formalist-standard-react",
  "version": "0.0.1",
  "description": "A standard form implementation for Formalist using React",
  "main": "lib/index.js",
  "scripts": {
    "compile": "./node_modules/.bin/babel --presets ./node_modules/babel-preset-es2015,./node_modules/babel-preset-react src --out-dir lib",
    "prepublish": "npm run compile",
    "test": "./node_modules/.bin/babel-node --presets ./node_modules/babel-preset-es2015,./node_modules/babel-preset-react test | faucet",
    "posttest": "npm run lint",
    "lint": "eslint src/*.js src/**/*.js test/*.js test/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/icelab/formalist-standard-react.git"
  },
  "keywords": [
    "formalist"
  ],
  "authors": [
    "Max Wheeler <max@icelab.com.au> (https://github.com/makenosound)",
    "Tim Riley <tim@icelab.com.au> (https://github.com/timriley)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/icelab/formalist-standard-react/issues"
  },
  "homepage": "https://github.com/icelab/formalist-standard-react",
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "enzyme": "^1.2.0",
    "eslint": "^1.10.3",
    "eslint-config-standard": "^4.4.0",
    "eslint-config-standard-react": "^1.2.1",
    "eslint-plugin-react": "^3.13.1",
    "eslint-plugin-standard": "^1.3.1",
    "faucet": "0.0.1",
    "react-addons-test-utils": "^0.14.5",
    "react-dom": "^0.14.5",
    "tape": "^4.2.2"
  },
  "dependencies": {
    "classnames": "^2.2.2",
    "formalist-compose": "file:../formalist-compose",
    "number-is-integer": "^1.0.0",
    "react": "^0.14.3",
    "react-immutable-proptypes": "^1.5.1"
  }
}
